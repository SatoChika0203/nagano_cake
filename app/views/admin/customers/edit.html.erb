<h1>admin/customer#edit</h1>

<h1>会員の編集画面</h1>

<%= form_with model: @customer, url:admin_customer_path(@customer.id), method: :patch do |f| %>
<p>last_name</p>
 <%= f.text_field :last_name %>
<p>first_name</p>
 <%= f.text_field :first_name %>
<p>last_name_kana</p>
 <%= f.text_field :last_name_kana %>
<p>first_name_kana</p>
 <%= f.text_field :first_name_kana %>
 <p>email</p>
 <%= f.text_field :email %>
<p>postal_code</p>
 <%= f.text_field :postal_code %>
<p>address</p>
 <%= f.text_area :address %>
<p>telephone_number</p>
 <%= f.text_field :telephone_number %>
<p>is_deleted</p>
 <%= f.text_field :is_deleted %>
 
   <%= f.submit 'Update Customer' %>
<% end %>